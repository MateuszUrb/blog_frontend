{
  "compilerOptions": {
    "target": "es2022",
    "moduleResolution": "node",
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": true,
    "lib": ["DOM", "es2022","dom.iterable"],
    "types": ["vitest/globals"]
  },
  "include": ["src/**/*", "./testSetup.js"],
"exclude": ["node_modules", "src/**/*.test.*"]
}
